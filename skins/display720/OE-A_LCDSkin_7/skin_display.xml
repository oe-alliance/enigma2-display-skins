			<convert type="TunerInfo">TunerUseMask</convert>
			<convert type="ValueBitTest">4</convert>
			<convert type="ConditionalShowHide" />
		</widget>
		<widget source="session.FrontendInfo" render="FixedLabel" position="80,500" size="25,70" text="C" font="FdLcDLight;60" halign="center" valign="bottom" foregroundColor="#00FF00" >
			<convert type="FrontendInfo">NUMBER</convert>
