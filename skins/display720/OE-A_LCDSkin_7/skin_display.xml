<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!--  Skin: LCD from Magic by MiLo -->
<!-- Skin is for et8500 LCD/TFT without panel widgets -->

<skin>
	<windowstyle id="1" type="skinned">
		<color color="#000000" name="Background"/>
		<color color="#ffffff" name="LabelForeground"/>
		<color color="#000000" name="ListboxBackground"/>
		<color color="#ffffff" name="ListboxForeground"/>
		<color color="#000000" name="ListboxSelectedBackground"/>
		<color color="#ffffff" name="ListboxSelectedForeground"/>
		<color color="#000000" name="ListboxMarkedBackground"/>
		<color color="#ffffff" name="ListboxMarkedForeground"/>
		<color color="#000000" name="ListboxMarkedAndSelectedBackground"/>
		<color color="#ffffff" name="ListboxMarkedAndSelectedForeground"/>
		<color color="#ffffff" name="WindowTitleForeground"/>
		<color color="#000000" name="WindowTitleBackground"/>
	</windowstyle>
	<fonts>
<!-- pick a funky name for the font, so we don't conflict  -->     
		<font filename="nmsbd.ttf" name="FdLcD" scale="90"/>
		<font filename="verdana.ttf" name="FdLcDLight" scale="90"/>
	</fonts>
	
<!-- template -->
	<screen name="SummaryClockPanel">
		<widget source="global.CurrentTime" render="Label" position="500,490" size="210,100" halign="right" valign="bottom" font="FdLcD;90" foregroundColor="#e5b243" >
			<convert type="ClockToText">Default</convert>
		</widget>
	</screen>

<!-- Tunerstatuspanel -->
	<screen name="SummaryTunerStatusPanel">
		<widget source="session.TunerInfo" render="FixedLabel" position="20,500" size="25,70" text="A" font="FdLcDLight;60" halign="center" valign="bottom" foregroundColor="#00FF00" >
			<convert type="TunerInfo">TunerUseMask</convert>
			<convert type="ValueBitTest">1</convert>
			<convert type="ConditionalShowHide" />
		</widget>
		<widget source="session.FrontendInfo" render="FixedLabel" position="20,500" size="25,70" text="A" font="FdLcDLight;60" halign="center" valign="bottom" foregroundColor="#00FF00">
			<convert type="FrontendInfo">NUMBER</convert>
			<convert type="ValueRange">0,0</convert>
			<convert type="ConditionalShowHide" />
		</widget>
		<widget source="session.TunerInfo" render="FixedLabel" position="50,500" size="25,70" text="B" font="FdLcDLight;60" halign="center" valign="bottom" foregroundColor="#00FF00" >
			<convert type="TunerInfo">TunerUseMask</convert>
			<convert type="ValueBitTest">2</convert>
			<convert type="ConditionalShowHide" />
